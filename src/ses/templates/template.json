{
    "TemplateName": "SwapALeaseMonitorTesting",
    "TemplateContent": {
        "Subject": "SwapALeaseMonitor: New Listings!",
        "Text": "New listings have been found!",
        "Html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <title>New attractive lease transfers<\/title>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <style>\r\n        .bold{\r\n            font-weight: bold;\r\n        }\r\n\r\n        h1 {\r\n            text-align: center;\r\n        }\r\n\r\n        .criteria > p {\r\n            text-align: center;\r\n            margin: 0;\r\n        }\r\n\r\n        .criteria > ul {\r\n            list-style: none;\r\n            text-align: center;\r\n            \r\n        }\r\n\r\n        .listings-container {\r\n            text-align: center;\r\n        }\r\n\r\n        .listing-item > h3 > a {\r\n            text-decoration: none;\r\n            color: #000;\r\n        }\r\n\r\n        .listing-item > ul {\r\n            list-style: none;\r\n        }\r\n\r\n    <\/style>\r\n<\/head>\r\n<body>\r\n    <div class=\"main\">\r\n    <div class=\"email-wrapper\">\r\n        <H1>We found {{listingCount}} new listings!<\/H1>\r\n        <section class=\"criteria\">  \r\n          <p class=\"bold\">Your Current criteria for listings:<\/p>\r\n          <ul>\r\n            <li>Zip: {{zip}}<\/li>\r\n            <li>Radius from zip: {{milesFromZip}} miles<\/li>\r\n            <li>Max months remaining on lease: {{maxMonthsRemaining}}<\/li>\r\n            <li>Max lease payment allowed: {{maxPayment}}<\/li> \r\n            <li>Minimum miles per month remaining on lease: {{minMilesRemaining}}<\/li>\r\n          <\/ul>\r\n        <\/section>\r\n        <section class=\"listings\">\r\n            <div class=\"listings-container\">\r\n                <h2>Listings:<\/h2>\r\n                <ul>\r\n                    {{#each listings}}\r\n                    <l1>\r\n                        <div class=\"listing-item\">\r\n                            <h3><a href=\"{{listingUrl}}\">{{title}}<\/a><\/h3>\r\n                            <img src={{imgUrl}}\/>\r\n                            <ul>\r\n                                <li>Monthly payment: {{costPerMonth}}<\/li>\r\n                                <li>Miles per month: {{milesRemaining}}<\/li>\r\n                                <li>Location: {{location}}<\/li>\r\n                                <li>Months remaining: {{monthsRemaining}}<\/li>\r\n                            <\/ul>\r\n                            <hr\/>\r\n                        <\/div>\r\n                    <\/l1>\r\n                    {{\/each}}\r\n                <\/ul>\r\n            <\/div>\r\n        <\/section>       \r\n    <\/div>\r\n    <\/div>\r\n<\/body>\r\n<\/html>"
    }
}
